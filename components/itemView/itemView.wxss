.item-navigator{
  width: 216rpx;
  margin-right: 20rpx;
  display: inline-block;
}

.item-navigator .item-group{
  width: 100%;
}

.item-navigator .item-group .thumbnail-group{
  width: 100%;
  height: 284rpx;
}

.item-navigator .item-group .thumbnail-group .thumbnail{
  width: 100%;
  height: 100%;
}

.item-navigator .item-group .item-title{
  width: 100%;
  height: 40rpx;
  text-align: center;
  font-size: 28rpx;
  margin-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}
